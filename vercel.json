{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"],
      "headers": {
        "Access-Controll-Allow-Origin": "*"
      }
    }
  ],
  "functions": {
    "api/**": {
      "maxDuration": 30
    }
  }
}
